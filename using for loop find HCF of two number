#include<stdio.h>                                 /* HCF of two integer */
int main()
{
  int a,b,i,hcf; 
  printf("enter value of two integers: ");
  scanf("%d %d",&a,&b);

    for(i=1; i<=a && i<=b; i++)
{        
    if(a%i==0 && b%i==0)
        hcf = i;
    
  }
    printf("hcf of %dand%dis%d",a,b,hcf);
  }  
  
